                            <div class="simple-dropdown widget-box-post-settings-dropdown">
                                <!-- SIMPLE DROPDOWN LINK -->
                                <p class="simple-dropdown-link">Widget Settings</p>
                                <!-- /SIMPLE DROPDOWN LINK -->
                            </div>
                            <!-- /SIMPLE DROPDOWN -->
                        </div>
                        <!-- /POST SETTINGS WRAP -->
                    </div>
                    <!-- /WIDGET BOX SETTINGS -->

                    <!-- WIDGET BOX TITLE -->
                    <p class="widget-box-title">Gallery<span class="highlighted"></span></p>
                    <!-- /WIDGET BOX TITLE -->

                    <!-- WIDGET BOX CONTENT -->
                    <div class="widget-box-content">
                        <!-- VIDEO BOX LIST -->
                        <div class="video-box-list">
                            <!-- VIDEO BOX -->
                            {% for m in medias[:5] %}

                                    <div class="video-box small">
                                        <!-- VIDEO BOX COVER -->
                                        <div class="video-box-cover popup-video-trigger">
                                            <!-- VIDEO BOX COVER IMAGE -->
                                            <figure class="video-box-cover-image liquid">
                                                {% if m.mediatype=="image" %}
                                                <img src="{{ asset('/uploads/Mixed') ~ m.source }}" alt="cover-08">
                                                 {% else %}
                                                 <video src="{{ asset('/uploads/Mixed') ~ m.source }}"></video>
                                                {% endif %}
                                            </figure>
                                            <!-- /VIDEO BOX COVER IMAGE -->


                                            <!-- VIDEO BOX INFO -->
                                            <div class="video-box-info">
                                                <!-- VIDEO BOX TITLE -->
                                                <p class="video-box-title">{{ m.idpost.titre }}</p>
                                                <!-- /VIDEO BOX TITLE -->

                                                <!-- VIDEO BOX TEXT -->
                                                <p class="video-box-text">{{ m.idpost.date|date }}</p>
                                                <!-- /VIDEO BOX TEXT -->
                                            </div>
                                            <!-- /VIDEO BOX INFO -->
                                        </div>
                                        <!-- /VIDEO BOX COVER -->
                                    </div>
                            {% endfor %}
                        </div>
                        <!-- /VIDEO BOX LIST -->
                    </div>
                    <!-- WIDGET BOX CONTENT -->
                </div>
                <!-- /WIDGET BOX -->

            </div>
            <!-- /GRID COLUMN -->

            <!-- GRID COLUMN -->
            <div class="grid-column">

                <div class="quick-post media">
                    <img class="mr-3" src="{{asset('template/img/avatar/')~app.user.imguser}}" alt="{{ app.user.username }}">
                    <div class="media-body">
                        <h6 class="mt-0">Créer une Publication</h6><br>
                        <input class="btn-primary rounded btn-lg btn-block popup-event-creation-trigger text-white" data-toggle="collapse" data-target="#nexpost"  aria-expanded="false" aria-controls="nexpost" title="Créer une Publication" placeholder="Salut  {{ app.user.username }} ! qu'est vous voulez dire?">
                    </div>
                </div>

                <div class="simple-tab-items">

                    <form class="form">

                        <div class="form-select">
                            <select id="newsfeed-filter-category" name="newsfeed_filter_category">
                                <option value="0">Profil de {{ utilisateur.username }}</option>
                            </select>
                            <!-- FORM SELECT ICON -->
                            <svg class="form-select-icon icon-small-arrow">
                                <use xlink:href="#svg-small-arrow"></use>
                            </svg>
                            <!-- /FORM SELECT ICON -->
                        </div>
                        <!-- /FORM SELECT -->
                    </form>
                    <!-- /FORM -->

                    <!-- SIMPLE TAB ITEM -->
                    <p class="simple-tab-item active">Publications</p>
                    <!-- /SIMPLE TAB ITEM -->
                </div>
                <!-- /SIMPLE TAB ITEMS -->
                {% for post in posts %}
                    {% if post.idauthor.id==utilisateur.id %}
                        <!-- WIDGET BOX -->
                        <div class="widget-box no-padding" id="post-{{ post.id }}">
                            <!-- WIDGET BOX SETTINGS -->
                            <div class="widget-box-settings">
                                <!-- POST SETTINGS WRAP -->
                                <div class="post-settings-wrap">
                                    {% if user %}
                                        <!-- POST SETTINGS -->
                                        <div class="post-settings widget-box-post-settings-dropdown-trigger">
                                            <!-- POST SETTINGS ICON -->
                                            <svg class="post-settings-icon icon-more-dots">
                                                <use xlink:href="#svg-more-dots"></use>
                                            </svg>
                                            <!-- /POST SETTINGS ICON -->
                                        </div>
                                        <!-- /POST SETTINGS -->

                                        <!-- SIMPLE DROPDOWN -->
                                        <div class="simple-dropdown widget-box-post-settings-dropdown">
                                            <!-- SIMPLE DROPDOWN LINK -->

                                            {% if post.idauthor.id==user.id %}
                                                <a class="simple-dropdown-link btn btn-default popup-album-creation-trigger" onclick="affichierpub(this)" id="ModifierPost-{{ post.id }}"data-whatever="{{ post.id }}" ><i class='fa fa-pencil'></i>Edit</a>
                                                <!-- /SIMPLE DROPDOWN LINK -->
                                                {% if post.type=="shared" %}
                                                    {% set f=0 %}
                                                    {% for po in posts %}
                                                        {% if post.contenue|number_format==po.id %}
                                                            {% set f=1 %}
                                                        {% endif %}
                                                    {% endfor %}
                                                    {% if f==1 %}
                                                        <a class="simple-dropdown-link btn btn-default"  data_but="btn-xs"  onclick="showpubforshare(this)" data-toggle="modal" data-target="#Postforshare" data-whatever="{{ post.contenue|number_format}}"><i class="fa fa-share-square-o" aria-hidden="true"></i>Share</a>
                                                    {% else %}
                                                        <a class="simple-dropdown-link disabled btn btn-default"  data_but="btn-xs"  onclick="showpubforshare(this)" data-toggle="modal" data-target="#Postforshare" data-whatever="{{ post.contenue|number_format}}"><i class="fa fa-share-square-o" aria-hidden="true" disabled ></i>Share</a>
                                                    {% endif %}
                                                {% else %}
                                                    <a class="simple-dropdown-link btn btn-default"  data_but="btn-xs"  onclick="showpubforshare(this)" data-toggle="modal" data-target="#Postforshare" data-whatever="{{ post.id }}"><i class="fa fa-share-square-o" aria-hidden="true"></i>Share</a>
                                                    <!-- SIMPLE DROPDOWN LINK -->
                                                {% endif %}
                                                <a class="simple-dropdown-link  btn btn-default"  data_but="btn-xs" href="{{ path('publication_delete',{ id_post: post.id})}}"><i class='fa fa-trash'></i>Delete</a>
                                                <a class="simple-dropdown-link  btn btn-default" data-toggle="collapse" href="#socialbuttons-{{ post.id }}" role="button" aria-expanded="false" aria-controls="collapseExample"><i class="fa fa-share-alt"></i>Social Media</a>

                                            {% else %}
                                                <!-- SIMPLE DROPDOWN LINK -->
                                                <p class="simple-dropdown-link">Make it Featured</p>
                                                <!-- /SIMPLE DROPDOWN LINK -->
                                                <a class="simple-dropdown-link btn btn-default"  data_but="btn-xs"  onclick="showpubforshare(this)"><i class='glyphicon glyphicon-share-alt'></i>share</a>
                                                <!-- SIMPLE DROPDOWN LINK -->
                                                <p class="simple-dropdown-link">Report Post</p>
                                                <!-- /SIMPLE DROPDOWN LINK -->
                                                <!-- SIMPLE DROPDOWN LINK -->
                                                <p class="simple-dropdown-link">Report Author</p>
                                                <!-- /SIMPLE DROPDOWN LINK -->
                                            {% endif %}


                                        </div>
                                        <!-- /SIMPLE DROPDOWN -->
                                    {% endif %}
                                </div>
                                <!-- /POST SETTINGS WRAP -->
                            </div>
                            <!-- /WIDGET BOX SETTINGS -->

                            <!-- WIDGET BOX STATUS -->
                            <div id="content-post{{ post.id }}"class="widget-box-status">
                                <!-- WIDGET BOX STATUS CONTENT -->
                                <div class="widget-box-status-content">
                                    <!-- USER STATUS -->
                                    <div class="user-status">
                                        <!-- USER STATUS AVATAR -->
                                        <a class="user-status-avatar" href="{{ path('visit_profile',{id_user:post.idauthor.id}) }}">
                                            <!-- USER AVATAR -->
                                            <div class="user-avatar small no-outline">
                                                <!-- USER AVATAR CONTENT -->
                                                <div class="user-avatar-content">
                                                    <!-- HEXAGON -->
                                                    <div class="hexagon-image-30-32" data-src="{{ asset('template/img/avatar/'~post.idauthor.imguser) }}"></div>
                                                    <!-- /HEXAGON -->
                                                </div>
                                                <!-- /USER AVATAR CONTENT -->

                                                <!-- USER AVATAR PROGRESS -->
                                                <div class="user-avatar-progress">
                                                    <!-- HEXAGON -->
                                                    <div class="hexagon-progress-40-44"></div>
                                                    <!-- /HEXAGON -->
                                                </div>
                                                <!-- /USER AVATAR PROGRESS -->

                                                <!-- USER AVATAR PROGRESS BORDER -->
                                                <div class="user-avatar-progress-border">
                                                    <!-- HEXAGON -->
                                                    <div class="hexagon-border-40-44"></div>
                                                    <!-- /HEXAGON -->
                                                </div>
                                                <!-- /USER AVATAR PROGRESS BORDER -->

                                                <!-- USER AVATAR BADGE -->
                                                <div class="user-avatar-badge">
                                                    <!-- USER AVATAR BADGE BORDER -->
                                                    <div class="user-avatar-badge-border">
                                                        <!-- HEXAGON -->
                                                        <div class="hexagon-22-24"></div>
                                                        <!-- /HEXAGON -->
                                                    </div>
                                                    <!-- /USER AVATAR BADGE BORDER -->

                                                    <!-- USER AVATAR BADGE CONTENT -->
                                                    <div class="user-avatar-badge-content">
                                                        <!-- HEXAGON -->
                                                        <div class="hexagon-dark-16-18"></div>
                                                        <!-- /HEXAGON -->
                                                    </div>
                                                    <!-- /USER AVATAR BADGE CONTENT -->

                                                    <!-- USER AVATAR BADGE TEXT -->
                                                    <p class="user-avatar-badge-text">12</p>
                                                    <!-- /USER AVATAR BADGE TEXT -->
                                                </div>
                                                <!-- /USER AVATAR BADGE -->
                                            </div>
                                            <!-- /USER AVATAR -->
                                        </a>
                                        <!-- /USER STATUS AVATAR -->

                                        <!-- USER STATUS TITLE -->
                                        <p class="user-status-title medium"><a class="bold" href="{{ path('visit_profile',{id_user:post.idauthor.id}) }}">{{post.idauthor.username}}</a> uploaded<span class="bold">a/an~{{ post.type }}~post</span></p>
                                        <!-- /USER STATUS TITLE -->

                                        <!-- USER STATUS TEXT -->
                                        <p class="user-status-text small">{% if "now" |date("U") + 3600  > post.date|date("U") + 31104000 %}
                                                {{ ("now" |date("U") + 3600  - post.date |date("U") ) |date("Y") }} Years Ago
                                            {% elseif "now" |date("U") + 3600  > post.date |date("U") + 2592000 %}
                                                {{ ("now" |date("U") + 3600  - post.date |date("U") ) |date("m") }} Months Ago
                                            {% elseif "now" |date("U") + 3600  > post.date |date("U") + 86400 %}
                                                {{ ("now" |date("U") + 3600  - post.date|date("U") ) |date("d") }} Days Ago
                                            {% elseif "now" |date("U") + 3600  > post.date |date("U") + 3600  %}
                                                {{ ("now" |date("U") + 3600  - post.date |date("U") ) |date("H") }} Hours Ago
                                            {% elseif "now" |date("U") + 3600  > post.date |date("U") + 60 %}
                                                {{ ("now" |date("U") + 3600  - post.date |date("U") ) |date("i") }} Minutes Ago
                                            {% elseif "now" |date("U") + 3600  > post.date |date("U")  %}
                                                {{ ("now" |date("U") + 3600  - post.date |date("U") ) |date("s") }} Seconds Ago
                                            {% endif %}</p>
                                        <!-- /USER STATUS TEXT -->
                                    </div>
                                    <!-- /USER STATUS -->

                                    <!-- WIDGET BOX STATUS TEXT -->
                                    <p class="widget-box-status-text">{{ post.titre }}</p>
                                    <!-- /WIDGET BOX STATUS TEXT -->
                                    {% if post.type=="Mixed" %}
                                        <h4 class="widget-box-status-text">{{ post.contenue }}</h4>
                                    {% endif %}
                                </div>
                                <!-- /WIDGET BOX STATUS CONTENT -->
                                {% set postmedias=[] %}
                                {% for m in medias %}
                                    {% if m.idpost.id==post.id %}
                                        {% set postmedias=postmedias|merge([m]) %}
                                    {% endif %}
                                {% endfor %}
                                <div class="picture-collage">
                                    {% if postmedias|length>0 %}
