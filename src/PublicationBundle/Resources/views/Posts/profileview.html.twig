                                                    <div class="picture-collage-item popup-picture-trigger">
                                                        <!-- PHOTO PREVIEW -->
                                                        <div class="photo-preview">
                                                            <!-- PHOTO PREVIEW IMAGE -->
                                                            <figure class="photo-preview-image liquid" >
                                                                {% if pm.mediatype=="image" %}
                                                                    <img src="{{ asset('uploads/Mixed/')~pm.source }}" alt="photo-preview-10" style="display: none;">
                                                                {% else %}
                                                                    <video src="{{ asset('uploads/Mixed/')~pm.source }}"></video>
                                                                {% endif %}
                                                            </figure>
                                                            <!-- /PHOTO PREVIEW IMAGE -->
                                                        </div>
                                                        <!-- /PHOTO PREVIEW -->
                                                    </div>
                                                    <!-- /PICTURE COLLAGE ITEM -->
                                                {% endfor %}
                                            </div>
                                            <!-- /PICTURE COLLAGE ROW -->
                                        {% else%}
                                            {% if postmedias|length>=2 and postmedias|length <= 4 %}
                                                <div class="picture-collage-row medium">
                                                    <!-- PICTURE COLLAGE ITEM -->
                                                    {% for pm in postmedias[:2] %}
                                                        <div class="picture-collage-item popup-picture-trigger">
                                                            <!-- PHOTO PREVIEW -->
                                                            <div class="photo-preview">
                                                                <!-- PHOTO PREVIEW IMAGE -->
                                                                <figure class="photo-preview-image liquid" >
                                                                    {% if pm.mediatype=="image" %}
                                                                        <img src="{{ asset('uploads/Mixed/')~pm.source }}" alt="photo-preview-10" style="display: none;">
                                                                    {% else %}
                                                                        <video src="{{ asset('uploads/Mixed/')~pm.source }}"></video>
                                                                    {% endif %}
                                                                </figure>
                                                                <!-- /PHOTO PREVIEW IMAGE -->
                                                            </div>

                                                        </div>
                                                    {% endfor %}
                                                </div>
                                                <!-- PICTURE COLLAGE ROW -->
                                                <div class="picture-collage-row">
                                                    {% for mp in postmedias[2:] %}
                                                        <!-- PICTURE COLLAGE ITEM -->
                                                        <div class="picture-collage-item popup-picture-trigger">
                                                            <!-- PHOTO PREVIEW -->
                                                            <div class="photo-preview">
                                                                <!-- PHOTO PREVIEW IMAGE -->
